# standard symbols

*e      :   T_END
*s      :   T_STRING
*i      :   T_IDENT
*n      :   T_INT
*f      :   T_FLOAT

*c,     :   T_COMMA
*c;     :   T_SEMICOLON
*c+     :   T_PLUS
*c/     :   T_DIV
*c-     :   T_MINUS
*c*     :   T_MUL
*c%     :   T_MOD
*c:     :   T_COLON
*c.     :   T_DOT
*c(     :   T_OPREN
*c)     :   T_CPREN
*c{     :   T_OCURLY
*c}     :   T_CCURLY

*d->    :   T_ARROW

chans : T_CHANS
gain : T_GAIN
mono : T_MONO
stereo : T_STEREO
pan : T_PAN
send : T_SEND
db : T_DB
ctrl : T_CTRL
in: T_IN
out: T_OUT
noconvert: T_NOCONVERT
smooth: T_SMOOTH
default: T_DEFAULT
fx: T_FX
master:T_MASTER
return: T_RETURN
prefade: T_PREFADE
postfade: T_POSTFADE
chain: T_CHAIN
params: T_PARAMS
min: T_MIN
max: T_MAX
plugins: T_PLUGINS
names: T_NAMES
from: T_FROM
